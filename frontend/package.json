{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.5",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "mobx-react-devtools": "^4.2.15",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0",
    "react-toastify": "^3.3.1",
    "reactstrap": "^4.8.0",
    "superagent": "^3.8.2"
  },
  "scripts": {
    "start": "yarn run build-css && yarn run watch-css & react-scripts start",
    "build": "yarn run build-css && react-scripts build",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "prettier":
      "./node_modules/prettier/bin/prettier.js --single-quote --no-semi --write 'src/**/*.js'",
